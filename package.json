{
  "name": "emberanvil",
  "version": "0.0.0",
  "private": "false",
  "scripts": {
    "dev": "vite",
    "update": "npm i -g npm-check-updates & ncu -u & npm install & npm update" ,
    "build": "vite build",
    "preview": "vite preview",
